{
    "name": "@jbcom/strata",
    "version": "1.0.0",
    "description": "Procedural 3D graphics library for React Three Fiber - terrain, water, vegetation, sky, volumetrics, and characters",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core/index.js"
        },
        "./components": {
            "types": "./dist/components/index.d.ts",
            "import": "./dist/components/index.js"
        },
        "./shaders": {
            "types": "./dist/shaders/index.d.ts",
            "import": "./dist/shaders/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js"
        },
        "./presets": {
            "types": "./dist/presets/index.d.ts",
            "import": "./dist/presets/index.js"
        },
        "./hooks": {
            "types": "./dist/hooks/index.d.ts",
            "import": "./dist/hooks/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rimraf dist",
        "docs:dev": "npx serve docs",
        "docs:build": "echo 'Static docs ready in docs/'",
        "docs:typedoc": "typedoc",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write src/ tests/",
        "format:check": "prettier --check src/ tests/",
        "test": "vitest run",
        "test:unit": "vitest run --config tests/unit/vitest.config.ts",
        "test:integration": "vitest run --config tests/integration/vitest.config.ts",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
        "prepublishOnly": "npm run build && npm run test:unit",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{js,json,md,yml,yaml}": [
            "prettier --write"
        ]
    },
    "keywords": [
        "strata",
        "3d-graphics",
        "procedural-generation",
        "three.js",
        "react-three-fiber",
        "r3f",
        "webgl",
        "sdf",
        "marching-cubes",
        "gpu-instancing",
        "raymarching",
        "volumetric",
        "terrain",
        "water",
        "fog",
        "vegetation",
        "fur",
        "character-animation",
        "shaders",
        "glsl"
    ],
    "author": "Jon Bogaty <jon@jonbogaty.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jbcom/strata"
    },
    "homepage": "https://github.com/jbcom/strata#readme",
    "bugs": {
        "url": "https://github.com/jbcom/strata/issues"
    },
    "dependencies": {
        "immer": "^11.0.1",
        "leva": "^0.10.1",
        "miniplex": "^2.0.0",
        "miniplex-react": "^2.0.0",
        "tunnel-rat": "^0.1.2",
        "zundo": "^2.3.0",
        "zustand": "^5.0.9"
    },
    "peerDependencies": {
        "@react-three/drei": ">=9.0.0",
        "@react-three/fiber": ">=8.0.0",
        "@react-three/rapier": ">=1.0.0",
        "postprocessing": ">=6.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.150.0",
        "yuka": ">=0.7.0",
        "zustand": ">=4.0.0"
    },
    "peerDependenciesMeta": {
        "yuka": {
            "optional": true
        },
        "@react-three/rapier": {
            "optional": true
        },
        "postprocessing": {
            "optional": true
        },
        "zustand": {
            "optional": true
        }
    },
    "devDependencies": {
        "@playwright/test": "^1.40.0",
        "@react-three/drei": "^9.114.0",
        "@react-three/fiber": "^8.15.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@testing-library/react": "^14.0.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/three": "^0.160.0",
        "@types/yuka": "^0.7.4",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "@vitest/coverage-v8": "^2.1.0",
        "eslint": "^8.56.0",
        "husky": "^9.0.0",
        "jsdom": "^27.2.0",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^5.0.0",
        "semantic-release": "^24.2.0",
        "three": "^0.160.0",
        "typedoc": "^0.26.0",
        "typescript": "^5.3.0",
        "vitest": "^2.1.0",
        "yuka": "^0.7.8"
    }
}
